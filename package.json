  "description": "",
  "main": "src/shared/http/server.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon -I --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/shared/http/server.ts",
    "start": "npm run build && node --experimental-specifier-resolution=node --loader ts-node/esm dist/shared/http/server.js",
    "docker:build": "docker build -t api-chatbot-riverdata .",
    "docker:run": "docker run -it -p 4009:4009 api-chatbot-riverdata",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [],
  "author": "Talita Araujo",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "node-llama-cpp": "^2.8.8",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.6",
    "@swc/plugin-transform-imports": "^1.5.105",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/nodemon": "^1.19.6",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.1",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "5.0.1",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "tsyringe": "^4.8.0",
    "typescript": "^5.4.2"
  }
